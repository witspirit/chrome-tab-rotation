{
  "manifest_version" : 2,

  "name" : "Tab Rotation",
  "description" : "Rotate between browser tabs",
  "version" : "0.1",

  "browser_action" : {
    "default_icon" : {
      "19" : "assets/images/RotatePaused-19.png",
      "38" : "assets/images/RotatePaused-38.png"
    },
    "default_title" : "Rotate !"
  },

  "background" : {
    "scripts" : ["js/main.js"],
    "persistent" : false
  },

  "permissions" : [
    "activeTab",
    "https://www.googleapis.com/"
  ]
}
